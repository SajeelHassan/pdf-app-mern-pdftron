{
  "name": "pdfapp",
  "version": "1.0.0",
  "description": "pdftron app",
  "main": "server.js",
  "scripts": {
    "client-install": "cd pdfbox && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "pdfbox": "npm start --prefix pdfbox",
    "dev": "concurrently \"npm run server\" \"npm run pdfbox\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm client-install npm install npm && run build"
  },
  "engines": {
    "node": "10.16.0"
  },
  "author": "Andrew",
  "license": "MIT",
  "dependencies": {
    "cloudinary": "^1.27.0",
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.1.3",
    "mongoose": "^6.0.10",
    "multer": "^1.4.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.13"
  }
}
